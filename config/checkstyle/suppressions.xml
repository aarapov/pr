<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.1//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>

    <suppress checks="JavadocMethod" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <suppress checks="MethodName" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <suppress checks="MethodLength" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <suppress checks="MagicNumber" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <suppress checks="JavadocPackage" files="(test.*)|Messages.java" />
    <suppress checks="ClassDataAbstractionCoupling" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <suppress checks="VisibilityModifier" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <suppress checks="AbstractClassName" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <suppress checks="EmptyBlockCheck" files=".*(Test.java$)|(Tests.java$)|(IT.java$)" />
    <!-- Do not analyze interfaces for GWT localizations -->
    <suppress checks=".*" files=".*Messages.java" />
    <!-- Do not analyze interfaces for GWT CSS resources -->
    <suppress checks=".*" files=".*Style.java" />
    <!-- Do not analize generated classes -->
    <suppress checks=".*" files=".*target.*" />
</suppressions>